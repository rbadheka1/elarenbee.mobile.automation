<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Elarenbee Mobile Automation Suite">
  <test name="Android Tests">
    <parameter name="platform" value="android" />
    <classes>
      <class name="com.elarenbee.mobile.automation.runner.TestRunner"/>
    </classes>
  </test>
  <test name="iOS Tests">
    <parameter name="platform" value="ios" />
    <classes>
      <class name="com.elarenbee.mobile.automation.runner.TestRunner"/>
    </classes>
  </test>
</suite>
